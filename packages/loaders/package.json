{
  "name": "@mpxjs/loaders",
  "version": "2.7.40",
  "description": "mpx loaders",
  "keywords": [
    "mpx"
  ],
  "author": "lareinayanyu",
  "license": "ISC",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "exports": {
    "./*": "./*",
    "./file-loader": {
      "types": "./dist/file-loader.d.ts",
      "require": "./dist/file-loader.js",
      "import": "./dist/file-loader.js"
    },
    "./pre-loader": {
      "types": "./dist/pre-loader.d.ts",
      "require": "./dist/pre-loader.js",
      "import": "./dist/pre-loader.js"
    },
    "./resolve-loader": {
      "types": "./dist/resolve-loader.d.ts",
      "require": "./dist/resolve-loader.js",
      "import": "./dist/resolve-loader.js"
    },
    "./selector": {
      "types": "./dist/selector.d.ts",
      "require": "./dist/selector.js",
      "import": "./dist/selector.js"
    },
    "./style-loader": {
      "types": "./dist/style-loader.d.ts",
      "require": "./dist/style-loader.js",
      "import": "./dist/style-loader.js"
    },
    "./url-loader": {
      "types": "./dist/url-loader.d.ts",
      "require": "./dist/url-loader.js",
      "import": "./dist/url-loader.js"
    }
  },
  "scripts": {
    "dev": "tsc -w",
    "build": "rimraf ./dist && tsc"
  },
  "dependencies": {
    "@babel/parser": "^7.16.2",
    "@babel/traverse": "^7.16.0",
    "@mpxjs/compiler": "^2.7.40",
    "@mpxjs/web-plugin": "^2.7.40",
    "@mpxjs/compile-utils": "^2.7.40",
    "@mpxjs/webpack-plugin": "^2.7.40",
    "json5": "^2.1.3",
    "loader-utils": "^2.0.2",
    "mime": "^2.6.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "eslint": "^7.28.0",
    "eslint-plugin-tsdoc": "^0.2.14"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:didi/mpx.git"
  },
  "homepage": "https://didi.github.io/mpx/",
  "bugs": {
    "url": "https://github.com/didi/mpx/issues"
  }
}
