<!-- 角色不符合主会场和跑跑预约礼 -->
<template>
  <view class="invalid-role-end">
    <error-page-paopao texts="{{texts}}" />
  </view>
</template>

<script>
  import { createComponent } from '@mpxjs/core'

  createComponent({
    properties: {
      admittance: {
        type: Boolean,
        value: true
      },
      backupTip: {
        type: Array,
        value: []
      }
    },
    computed: {
      texts () {
        if (this.admittance === false) {
          return ['抱歉，您当前账号未激活', '请更换账号重新尝试', '已注册的滴滴司机请用接单账号进行登录']
        }
        if (this.backupTip && this.backupTip.length) {
          return this.backupTip
        }
        return ['仅限快车、优享、专车、轻快、', '橙选、滴滴定制车司机', '可参与此活动']
      }
    }
  })
</script>

<style lang="stylus">
</style>

<script type="application/json">
  {
    "component": true,
    "usingComponents": {
      "error-page-paopao": "./error-page-paopao"
    }
  }
</script>
