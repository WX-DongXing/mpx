<!-- 只是用来作为独立小程序测试的，主包已经提供 webview 组件 -->
<template>
  <web-view src="{{ linkUrl }}"></web-view>
</template>

<script>
import { createPage } from '@mpxjs/core'

createPage({
  data: {
    linkUrl: ''
  },
  onLoad (query) {
    if (query.url) {
      this.linkUrl = decodeURIComponent(query.url)
    }
  }
})
</script>
